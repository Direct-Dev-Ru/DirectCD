{
    "job_name": "main ddru (assets)",
    "job_type": "tags-prefixed",
    "check_interval": 120,

    "git_repo_url": "git@github.com:Direct-Dev-Ru/http2-nodejs-ddru.git",
    "git_private_key": "/root/.ssh/id_rsa",
    "git_start_tag": "v0.0.0",
    "git_max_tag": "v1.99.99",
    "git_tag_prefix": "assets-",
    "git_branch": "assets",
    "git_start_tag_file": "/tmp/cdddru/start-tag-file",
    "local_git_folder": "/tmp/git-local-repo",
    "git_commit": "",

    "do_docker_build": false,
    "docker_file": "",
    "docker_image": "",
    "docker_server": "",
    "docker_user": "",
    "docker_token": "",    
    
    "do_subfolder_sync": true,
    "git_sub_folder": "/assets/",
    "target_folder": "/app/ext-data",
    
    "do_manifest_deploy": false,
    "manifests_k8s": "/app/manifests/k8s-main-site-manifests.yaml",
    "deployment_name_k8s": "main-site",
    "namespace_k8s": "test-app",
    "context_k8s": "default"
}